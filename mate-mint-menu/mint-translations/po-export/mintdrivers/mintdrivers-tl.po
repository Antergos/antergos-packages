#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:33 generate_desktop_files:26
msgid "Driver Manager"
msgstr "Ayusin ang Driver"

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:43
msgid ""
"Drivers cannot be installed.\n"
"Please connect to the Internet or insert the Linux Mint installation DVD (or "
"USB stick)."
msgstr ""
"Mga driver ay hindi maaaring i-install.\n"
"Mangyaring kumonekta sa Internet o ipasok ang DVD-install ng Linux Mint (o "
"USB stick)."

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:126
msgid "Applying changes..."
msgstr "Ang paglalapat ng pagbabago..."

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:218
msgid "Re_vert"
msgstr "Ba_ligtarin"

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:220
msgid "_Apply Changes"
msgstr "_Ilapat ang mga pagbabago"

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:222
msgid "_Cancel"
msgstr "_kanselahin"

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:224
msgid "_Restart..."
msgstr "_Simulan muli..."

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:307
msgid "This device is using the recommended driver."
msgstr "Ang aparatong ito ay gumagamit ng controller inirerekumenda."

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:308
msgid "This device is using an alternative driver."
msgstr "Ang aparatong ito ay gumagamit ng isang kahaliling controller."

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:309
msgid "This device is using a manually-installed driver."
msgstr "Ang aparatong ito ay gumagamit ng controller manu-mano-install."

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:310
msgid "This device is not working."
msgstr "Ang aparatong na ito ay hindi gumagana."

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:319
msgid "Continue using a manually installed driver"
msgstr "Magpatuloy gamit ang isang controller manu-mano-install"

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:346
msgid "recommended"
msgstr "Iminungkahi"

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:346
#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:348
msgid "Version"
msgstr "Bersyon"

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:370
msgid "Do not use the device"
msgstr "Huwag gamitin ang aparato"

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:388
#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:389
#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:419
msgid "Unknown"
msgstr "Hindi alam"

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:472
msgid "You need to restart the computer to complete the driver changes."
msgstr ""
"Kailangan mong i-restart ang computer upang makumpleto ang pagbabago ng "
"driver."

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:486
#, python-format
msgid "%(count)d proprietary driver in use."
msgid_plural "%(count)d proprietary drivers in use."
msgstr[0] "%(count)d driver may-ari na ginagamit."
msgstr[1] "%(count)d pinagmamay-arian Controllers gagamitin."

#: usr/lib/linuxmint/mintDrivers/mintDrivers.py:491
msgid "No proprietary drivers are in use."
msgstr "No proprietary drivers are in use."

#: usr/bin/driver-manager:9
msgid "Please enter your password to launch the driver manager"
msgstr ""
"Mangyaring ipasok ang iyong password upang simulan ang pag-configure ng "
"controller"

#: generate_desktop_files:26
msgid "Manage the drivers for your devices"
msgstr "Pamahalaan ang controllers sa kanilang mga aparato"
